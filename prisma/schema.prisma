datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

// tabela User
// @ => primary key
// @@map => nome da tabela
model User {
  id         String @id @default(uuid())
  name       String
  github_id  Int
  avatar_url String
  login      String

  @@map("users")
}
